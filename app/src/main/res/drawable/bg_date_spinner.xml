<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid
                android:color="@color/white" />
            <corners
                android:radius="6dp" />
            <stroke
                android:width="1dp"
                android:color="@color/borderColor" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_calendar_icon"
        android:bottom="10dp"
        android:gravity="start|center"
        android:width="20dp"
        android:height="20dp"
        android:left="10dp"
        android:top="10dp" />
    <item
        android:drawable="@drawable/ic_chevron_down_icon"
        android:bottom="10dp"
        android:height="9.5dp"
        android:width="16.8dp"
        android:gravity="end|center"
        android:right="10dp"
        android:top="10dp" />
</layer-list>